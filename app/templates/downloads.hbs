          <th class="table__header downloads__table__header">{{ t 'downloads.thead_muni' }}</th>
        </tr>
        <tr class="table__row table__row--body">
          <td class="table__cell">{{ t 'downloads.trade_row_1' }}</td>
          <td class="table__cell">{{ t 'downloads.cta_na' }}</td>
          <td class="table__cell">
            <a href="https://s3.amazonaws.com/datlas-colombia-downloads/189f86fc98d07ed9069b1ca9c1dfcdacce57d142/products_department.csv" class="link--stream link--stream--inline--small">{{ t 'downloads.cta_download' }}</a>
