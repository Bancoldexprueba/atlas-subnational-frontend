{{#unless isFiltered }}
<h3 class="section__head">{{setTitle}}</h3>
  {{component 'multiples-graph'
    classNames='buildermod__viz'
    immutableData=totalSet
    data=totalSet
    dataType=dataType
    varDependent=varDependent
    startDate=startDate
    dateExtent=dateExtent
    endDate=endDate
    markerGroup='total'
  }}
{{/unless}}

{{#if showParent}}
  <h3 class="section__head">{{parentBreadcrumb}}</h3>
  {{component 'multiples-graph'
    classNames='buildermod__viz'
    immutableData=parentSet
    data=parentSet
    dataType=dataType
    varDependent=varDependent
    startDate=startDate
    dateExtent=dateExtent
    endDate=endDate
    markerGroup='parent'
  }}
{{/if}}

<h3 class="section__head">{{childBreadcrumb}}</h3>
{{component 'multiples-graph'
  classNames='buildermod__viz'
  immutableData=immutableData
  data=data
  dataType=dataType
  varDependent=varDependent
  startDate=startDate
  dateExtent=dateExtent
  endDate=endDate
  markerGroup='subsets'
}}

