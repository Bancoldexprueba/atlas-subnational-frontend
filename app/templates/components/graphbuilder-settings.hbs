<form class="buildermod__drawer buildermod__drawer--settings">
  <fieldset class="settings__input__wrap">
{{!--       {{input-time-selects
        elementId="settings__input--time"
        type="time"
        startDate=startDate
        endDate=endDate
        newStartDate=newStartDate
        newEndDate=newEndDate
        dateExtent=dateExtent
        dateRange=dateRange
        content=dateRange
        selectedValue=startDate
      }} --}}
      <label for="settings__input--time" class="settings__input">{{t 'graph_builder.settings.change_time'}}</label>
      <select class="settings__input--select" data-dateIndex="start" {{action 'changeStart' on='change'}}>
        {{#each dateRange key="@index" as |item|}}
          <option value="{{item}}" selected={{is-equal item startDate}}>
            {{item}}
          </option>
        {{/each}}
      </select>
      <span class="settings__input__text">to</span>
      <select class="settings__input--select" data-dateIndex="end" {{action 'changeEnd' on='change'}}>
        {{#each dateRange key="@index" as |item|}}
          <option value="{{item}}" selected={{is-equal item endDate}}>
            {{item}}
          </option>
        {{/each}}
      </select>
  </fieldset>
</form>

