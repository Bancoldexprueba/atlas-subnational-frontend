{{#if locationResults.length}}
  <h3 class="results__header">Results: Locations</h3>
  <ul class="search__results">
    {{#each result in locationResults}}
      {{search-result
        result=result
        tagName="li"
        class="search__result"
      }}
    {{/each}}
  </ul>
{{/if}}
{{#if productResults.length}}
